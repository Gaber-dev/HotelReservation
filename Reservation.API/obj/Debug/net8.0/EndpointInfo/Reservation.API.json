        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/FooterInfo"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FooterInfo"
                }
              },
              "text/json": {
                "schema": {
